{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-param-reassign": 0,
    "strict": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "global-require": 0,
    "no-restricted-syntax": 0,
    "no-console": 0,
    "guard-for-in": 0,
    "new-cap": 0,
    "no-mixed-operators": 0,
    "max-len": 0,
    "consistent-return": 0,
    "no-new": 0,
    "no-eval": 0,
    "linebreak-style": 0,
    "no-extend-native": 0,
    "spaced-comment": 0,
    "no-plusplus": 0,
    "comma-dangle": 0,
    "no-await-in-loop": 0,
    "object-curly-newline": 0,
    "import/named": 0,
    "arrow-parens": 0,
    "prefer-destructuring": "warn"
  }
}
